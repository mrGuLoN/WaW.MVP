				float Metallic = ( tex2DNode142.r * _MetallicMultiplier );
				float Smoothness = ( tex2DNode142.a * _SmoothnessMultiplier );
				float Occlusion = ( tex2D( _Occlusion, uv_Occlusion ).r * _OcclusionMultiplier );
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;
				float3 BakedGI = 0;
				float3 RefractionColor = 1;
				float RefractionIndex = 1;
				float3 Transmission = 1;
				float3 Translucency = 1;

