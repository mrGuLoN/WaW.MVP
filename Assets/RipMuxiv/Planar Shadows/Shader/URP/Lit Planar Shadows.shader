				float3 WorldViewDirection = _WorldSpaceCameraPos.xyz  - WorldPosition;
				float4 ShadowCoords = float4( 0, 0, 0, 0 );
				float4 ClipPos = input.clipPosV;
				float4 ScreenPos = ComputeScreenPos( input.clipPosV );

				float2 NormalizedScreenSpaceUV = GetNormalizedScreenSpaceUV(input.positionCS);

				#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
					ShadowCoords = input.shadowCoord;
				#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
					ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
				#else
					ShadowCoords = float4(0, 0, 0, 0);
				#endif

				WorldViewDirection = SafeNormalize( WorldViewDirection );

				float2 uv_MainTex = input.ase_texcoord8.xy * _MainTex_ST.xy + _MainTex_ST.zw;
				
				float2 uv_Normal = input.ase_texcoord8.xy * _Normal_ST.xy + _Normal_ST.zw;
				float3 unpack141 = UnpackNormalScale( tex2D( _Normal, uv_Normal ), _NormalScale );
				unpack141.z = lerp( 1, unpack141.z, saturate(_NormalScale) );
				
				float2 uv_Emission = input.ase_texcoord8.xy * _Emission_ST.xy + _Emission_ST.zw;
				
				float2 uv_MetallicSmoothness = input.ase_texcoord8.xy * _MetallicSmoothness_ST.xy + _MetallicSmoothness_ST.zw;
				float4 tex2DNode142 = tex2D( _MetallicSmoothness, uv_MetallicSmoothness );
				
